page{  
  display: flex;  
  flex-direction: column;  
  height: 100%;  
}  

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0;
  box-sizing: border-box;
}

.textStyle {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.textContainer {
  margin-top: 200px;
}

.navbar{
  white-space: nowrap;
  height: 100rpx;
  width: 100%;
  background-color: #f5f5f5;
  text-align:center;
  color:#5b5b5b;
}

.navcell{
  box-sizing: content-box;
  font-size: 24rpx;
  line-height: 100rpx;
  text-align: center;
  display: inline-block;
  overflow: hidden;
}

.nav-item{
  border-bottom: 3rpx solid #fff;
}

.nav-item-on{
  border-bottom: 3rpx solid #11cd6e;
  color: #11cd6e;
}

.navigator-hover{
  border-bottom: 3rpx solid #11cd6e;
  color: #11cd6e;
}